{
    "name": "schoolsquirrel",
    "version": "0.0.1",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build --prod",
        "test": "ng test",
        "lint": "eslint . --ext .ts",
        "lint-fix": "eslint . --ext .ts --fix",
        "e2e": "ng e2e",
        "crazy-clean": "npx rimraf hooks node_modules platforms package-lock.json && npm i",
        "clean": "npx rimraf hooks platforms package-lock.json",
        "android": "tns debug android --env.aot --no-hmr",
        "android:prod": "tns build android --env.aot",
        "ios": "tns debug ios --env.aot --no-hmr",
        "mobile": "tns run --env.aot --no-hmr",
        "preview": "tns preview --env.aot --no-hmr",
        "desktop": "wait-on http-get://localhost:4200/ && npm run electron:tsc && electron ./ --serve",
        "electron:tsc": "tsc electron-main.ts",
        "ngcc": "ngcc --properties es2015 module main --first-only",
        "postinstall": "npm run ngcc"
    },
    "main": "electron-main.js",
    "private": true,
    "dependencies": {
